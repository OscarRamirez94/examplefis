// Cargar la librerÃ­a de cheerio para parsear XML
let $ = cheerio.load(pm.response.text(), { xmlMode: true });

// Buscar el valor de StatusCode
let statusCode = $("v1\\:StatusCode").text();

// Mostrar en la consola de Postman
console.log("ðŸ‘‰ StatusCode =", statusCode);

// Guardarlo como variable (por si lo necesitas despuÃ©s)
pm.collectionVariables.set("statusCode", statusCode);

// Hacer un test de ejemplo
pm.test("El response trae StatusCode", function () {
    pm.expect(statusCode).to.not.be.empty;
});